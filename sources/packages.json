{
	"packages": [
		{
			"name": "bash",
			"version": "5.3",
			"download_url": "https://mirrors.ibiblio.org/gnu/bash/",
			"configuring_cmd": [
				"configure",
				"--build=${BUILDMACHINE}",
				"--host=${CHOST}",
				"--prefix=/usr"
			],
			"building_cmd": [
				"make",
				"-j$(nproc)"
			],
			"installing_cmd": [
				"make",
				"DESTDIR=${ROOT_DIR}",
				"install"
			]
		},
		{
			"name": "coreutils",
			"version": "9.8",
			"download_url": "https://mirror.its.dal.ca/gnu/coreutils/",
			"configuring_cmd": [
				"configure",
				"--build=${BUILDMACHINE}",
				"--host=${CHOST}",
				"--without-selinux",
				"--disable-libcap",
				"--prefix=/usr"
			],
			"building_cmd": [
				"make",
				"-j$(nproc)"
			],
			"installing_cmd": [
				"make",
				"DESTDIR=${ROOT_DIR}",
				"install"
			]
		},
		{
			"name": "util-linux",
			"version": "2.41.2",
			"download_url": "https://www.kernel.org/pub/linux/utils/util-linux/v2.41/",
			"configuring_cmd": [
				"configure",
				"--build=${BUILDMACHINE}",
				"--host=${CHOST}",
				"--disable-liblastlog2",
				"--prefix=/usr"
			],
			"building_cmd": [
				"make",
				"-j$(nproc)"
			],
			"installing_cmd": [
				"make",
				"DESTDIR=${ROOT_DIR}",
				"install"
			]
		},
		{
			"name": "nano",
			"version": "8.6",
			"download_url": "https://mirrors.ocf.berkeley.edu/gnu/nano/",
			"configuring_cmd": [
				"configure",
				"--build=${BUILDMACHINE}",
				"--host=${CHOST}",
				"--prefix=/usr"
			],
			"building_cmd": [
				"make",
				"-j$(nproc)"
			],
			"installing_cmd": [
				"make",
				"DESTDIR=${ROOT_DIR}",
				"install"
			]
		}
	]
}